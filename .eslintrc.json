{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-unresolved": "off",
        "import/extensions": "off",
        "no-console": "off",
        "quotes": ["error", "double"],
        "no-shadow": "off",
        "no-underscore-dangle": "off",
        "linebreak-style":"off",
        "no-plusplus":"off"
    }
}
