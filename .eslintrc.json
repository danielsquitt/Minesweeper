{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
    ],
    "settings": {
        "import/extensions": [
          ".ts"
        ]
      },
    "rules": {
        "no-plusplus":"off",
        "no-restricted-syntax":"off",
        "lines-between-class-members":"off",
        "import/extensions":"off",
        "import/no-unresolved": 2
    }
}
